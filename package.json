{
  "name": "agro-mas-frontend",
  "version": "1.0.0",
  "description": "Frontend Angular para Agro Mas - Marketplace Agropecuario",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
